{
    "name": "workofstan/backyard",
    "description": "Library of useful functions and classes",
    "type": "library",
    "homepage": "https://github.com/WorkOfStan/backyard",
    "support": {
        "issues": "https://github.com/WorkOfStan/backyard/issues",
        "source": "https://github.com/WorkOfStan/backyard"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Stanislav Rejthar",
            "email": "rejthar@stanislavrejthar.com",
            "homepage": "http://www.stanislavrejthar.com/"
        }
    ],
    "require": {
        "php": "^5.3 || ^7.0",
        "ext-curl" : "*",
        "psr/log": "^1.0 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4|^5",
        "phpstan/phpstan-webmozart-assert": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "WorkOfStan\\Backyard\\": "classes/"
        },
        "files": [
            "classes/ThrowableFunctions/ThrowablePHPFunctions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WorkOfStan\\Backyard\\Test\\": "Test/"
        }
    }
}
